{
  "name": "helpharma-express-ts",
  "version": "0.0.1",
  "description": "Helpharma backend made with express, typescript and typeorm",
  "author": "John Jairo Garc√≠a <johnjagarcia94@gmail.com>",
  "devDependencies": {
    "@types/node": "13.9.5",
    "@types/readable-stream": "2.3.5",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-eslint": "^10.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "pm2": "^4.2.3",
    "ts-node": "^8.8.1",
    "tsc-watch": "^4.2.3",
    "tsconfig-paths": "^3.9.0",
    "typeorm-model-generator": "^0.4.1",
    "typescript": "3.6.3"
  },
  "dependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/helmet": "0.0.45",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/socket.io": "^2.1.4",
    "amqplib": "^0.5.5",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "class-validator": "^0.11.1",
    "colombia-holidays": "0.0.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "express": "^4.17.1",
    "express-queue": "0.0.12",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongodb": "^3.5.7",
    "mssql": "^6.2.0",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "nodemailer": "^6.4.6",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "socket.io": "^2.3.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslib": "1.11.2",
    "typeorm": "0.2.19",
    "xlsx": "^0.15.6"
  },
  "scripts": {
    "tsc": "tsc -t ES5",
    "build": "npm-run-all tsc",
    "dev:start": "npm-run-all build start",
    "start": "ts-node src/index.ts",
    "dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/index.js\"",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "ts-node ./node_modules/.bin/typeorm migration:create -n",
    "migration:run": "mv src/entity src/0e; ts-node ./node_modules/.bin/typeorm migration:run; mv src/0e src/entity;",
    "migration:revert": "mv src/entity src/0e; ts-node ./node_modules/.bin/typeorm migration:revert; mv src/0e src/entity;",
    "pm2:start": "rm ormconfig.json && cp ormconfig.json.prod ormconfig.json && pm2 install typescript && pm2 start src/index.ts",
    "pm2:list": "pm2 list",
    "pm2:stop": "pm2 stop 0",
    "pm2:logs": "pm2 logs 0 --lines 100"
  }
}
